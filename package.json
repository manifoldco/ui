{
  "name": "embeddable-marketplace",
  "private": true,
  "version": "0.0.1",
  "description": "Manifold Embeddable Marketplace",
  "scripts": {
    "build": "stencil build",
    "format": "npm run format:ts && npm run format:css",
    "format:css": "stylelint 'src/**/*.css' --fix",
    "format:css:watch": "npx onchange 'src/**/*.css' -- npm run format:css",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:watch": "npx onchange 'src/**/*.{css,ts,tsx}' -- npm run format",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.ts,.tsx src",
    "lint:css": "stylelint 'src/**/*.css'",
    "start": "stencil build --dev --docs --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:coverage": "stencil test --spec --e2e --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/core": "~0.16.2",
    "@stencil/postcss": "^0.1.0",
    "@stencil/router": "~0.3.1",
    "@typescript-eslint/parser": "^1.1.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "postcss-preset-env": "^6.5.0",
    "puppeteer": "1.8.0",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "0.0.4",
    "stylelint-config-standard": "^18.2.0"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/puppeteer": "1.11.2",
    "prettier": "^1.16.2"
  }
}

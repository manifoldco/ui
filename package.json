{
  "name": "@manifoldco/ui",
  "description": "Manifold UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldco/ui.git"
  },
  "homepage": "https://github.com/manifoldco/ui",
  "bugs": {
    "url": "https://github.com/manifoldco/ui/issues"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "author": "manifoldco",
  "license": "ISC",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "esnext": "dist/esm",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "build": "npm run build:docs && cd docs && npm run build",
    "build:docs": "rm -rf docs/node_modules/@manifoldco/ui && mkdir docs/node_modules/@manifoldco/ui && cp package.json docs/node_modules/@manifoldco/ui && stencil build && cp -r dist docs/node_modules/@manifoldco/ui",
    "build-storybook": "stencil build && build-storybook",
    "dev": "concurrently 'stencil build --watch' 'wait-on dist/loader/index.cjs.js && cp .manifold-local.yml .manifold.yml && manifold run -- npm run storybook'",
    "docs": "npm run build:docs && cd docs && npm start",
    "generate:gql": "graphql-codegen --config codegen.yml",
    "generate:specs": "npm run generate:specs:catalog && npm run generate:specs:gateway && npm run generate:specs:marketplace && npm run generate:specs:provisioning && npm run generate:specs:connector",
    "generate:specs:catalog": "npx @manifoldco/swagger-to-ts src/spec/catalog/v1.yaml -w 'export namespace Catalog' -o src/types/catalog.ts",
    "generate:specs:gateway": "npx @manifoldco/swagger-to-ts src/spec/gateway/v1.yaml -w 'export namespace Gateway' -o src/types/gateway.ts",
    "generate:specs:marketplace": "npx @manifoldco/swagger-to-ts src/spec/marketplace/v1.yaml -w 'export namespace Marketplace' -o src/types/marketplace.ts",
    "generate:specs:provisioning": "npx @manifoldco/swagger-to-ts src/spec/provisioning/v1.yaml -w 'export namespace Provisioning' -o src/types/provisioning.ts",
    "generate:specs:connector": "npx @manifoldco/swagger-to-ts src/spec/connector/v1.yaml -w 'export namespace Connector' -o src/types/connector.ts",
    "happo": "stencil build && cp .manifold-ci.yml .manifold.yml && manifold run -- happo run",
    "happo-ci-travis": "stencil build && happo-ci-travis",
    "lint": "npm run lint:js && npm run lint:css && cd docs && npm run lint",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --ext .js,.ts,.tsx src",
    "now-build": "npm run build-storybook",
    "postinstall": "cd docs && npm i",
    "prepare": "stencil build",
    "publish": "node scripts/publish",
    "storybook": "start-storybook -p 6006",
    "test": "stencil test --spec --e2e",
    "test:e2e": "stencil test --e2e",
    "test:spec": "stencil test --spec",
    "test:coverage": "stencil test --spec --e2e --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/stencil test --spec --e2e --runInBand",
    "test:watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@manifoldco/gql-zero": "^0.2.0",
    "@manifoldco/shadowcat": "^0.1.5",
    "@stencil/state-tunnel": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/introspection": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@graphql-codegen/typescript-operations": "^1.3.1",
    "@manifoldco/eslint-plugin-stencil": "^0.2.1",
    "@manifoldco/icons": "0.0.3",
    "@reach/observe-rect": "^1.0.3",
    "@stencil/core": "^1.2.2",
    "@stencil/postcss": "^1.0.1",
    "@storybook/addons": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.19.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "core-js": "^3.1.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "fetch-mock": "^7.3.9",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "happo-plugin-typescript": "^1.0.0",
    "happo.io": "^3.20.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "puppeteer": "1.17.0",
    "storybook-readme": "^5.0.5",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^6.0.4",
    "wait-on": "^3.3.0",
    "webpack": "^4.38.0"
  }
}
